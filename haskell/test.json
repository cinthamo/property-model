{
    "@definitions": {
        "one": {
            "@type": "^number",
            "@aspects": [{
                "resolver": "^Resolvers.Default"
            }],
            "default": 3
        },
        "two": {
            "@type": "^number",
            "@aspects": [{
                "resolver": "^Resolvers.Default"
            }],
            "default": {
                "@type": "^case",
                "rules": [{
                    "if": {
                        "f": "^equal",
                        "p": ["^one", 1]
                    },
                    "value": 2
                }],
                "else": {
                    "f": "^add",
                    "p": [{
                            "f": "^add",
                            "p": ["^one", -1]
                        },
                        4
                    ]
                }
            }
        }
    }
}